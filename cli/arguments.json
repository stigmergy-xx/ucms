{
  "useCommand": true,
  "defaultCommand": "development",
  "commands": {
    "development": {
      "options": {
        "development":{
          "default": true
        }
      }
    },
    "production": {
    }
  },
  "globalOptions": {
    "port": {
      "default": 8901,
      "hasValue": true,
